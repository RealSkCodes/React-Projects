{
  "version": 2,
  "builds": [
    {
      "src": "api/swiggyMainApi.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/swiggyRestDataApi.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/swiggyMainApi",
      "dest": "/api/swiggyMainApi.js"
    },
    {
      "src": "/api/swiggyRestDataApi",
      "dest": "/api/swiggyRestDataApi.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
